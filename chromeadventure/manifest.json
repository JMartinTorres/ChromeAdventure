{
  "manifest_version": 2,
  "name": "Chrome Adventure",
  "description": "We're going on a trip!",
  "short_name": "ChromeAdventure",
  "version": "1.0.0.0",

  "icons": {
    "16": "extension_icons/editionmode-active-16.png",
    "32": "extension_icons/editionmode-active-32.png",
    "48": "extension_icons/editionmode-active-48.png",
    "128": "extension_icons/editionmode-active-128.png"
  },

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery/jquery-3.4.1.min.js",
        "content_scripts/start_adventure.js"
      ]
    }
  ]
}